@{
    ViewBag.Title = "DailyLog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<link href="~/Content/DataTable/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/Scripts/DataTable/jquery.dataTables.min.js"></script>
<script src="~/Scripts/AppJs/DailyLog.js"></script>
<div class="contentpanel">
    <div class="row">
        <div class="col-md-12">
            <div class="row" style="background:#19AFC6;">
                <div class="col-md-6"><h5 class="adminhead">DailyLog Details</h5></div>
                <div class="col-md-6 text-right">
                    @{if (@ViewBag.CreatePermission)
                        {
                            <button class="btn btn-default btn-xs mt5 mb-xs" onclick="CreateUpdateDailyLogPopup(0)">Create DailyLog</button>
                        }
                    }
                </div>
            </div>
            <div class="panel-body table-responsive">
                <table id="tbldailylog" class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>Customer</th>
                            <th>Guard</th>
                            <th>Hours</th>
                            <th>Date</th>
                            <th>Comments</th>
                            <th>Created On</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
