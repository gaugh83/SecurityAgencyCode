@{
    ViewBag.Title = "Customer Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/DataTable/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/Scripts/DataTable/jquery.dataTables.min.js"></script>
<script src="~/Scripts/AppJs/Customer.js"></script>
<div class="contentpanel">
    <div class="row">
        <div class="col-md-12">
            <div class="row" style="background:#19AFC6;">
                <div class="col-md-12"><h5 class="adminhead">Customer Report</h5></div>
            </div>
            <div class="row" style="margin-top:20px">
                <div class="col-md-3">
                    Registered Between
                </div>
                <div class="col-md-3">
                    Start Date: <input type="date" id="StartDate" /> 
                </div>
                <div class="col-md-3">
                    End Date: <input type="date" id="EndDate" />
                </div>
                <div class="col-md-3">
                    <input type="button" value="View Report" class="btn btn-primary" onclick="GenerateCustomerReport()" />
                </div>
            </div>
        </div>
    </div>
</div>

